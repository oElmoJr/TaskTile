{
  "expo": {
    "name": "TaskTile",
    "slug": "TaskTile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/bento-grid-mono-icon.png",
    "scheme": "tasktile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/bento-grid-mono-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/bento-grid-mono-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "dev.elmojr.tasktile"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "eaf03b87-e96a-4805-b59c-ebc92703b609"
      }
    }
  }
}